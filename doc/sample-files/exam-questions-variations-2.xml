<?xml version="1.0" encoding="UTF-8"?>

<exam xmlns="http://www.it.uc3m.es/jaf/eyegrade/ns/"
      xmlns:eye="http://www.it.uc3m.es/jaf/eyegrade/ns/">

  <subject>Mathematics</subject>
  <degree>Bachelor's Degree in Mathematics</degree>
  <title>Final examination</title>
  <date>January 1st, 2020</date>
  <duration>10 min.</duration>
  <studentId eye:label="ID" eye:length="8" />
  <scores eye:maxScore="100" eye:penalize="false" />

  <question>
    <!-- Q1 -->
    <variation>
      <text>
        Given the set $A = \{a, b, c, d\}$,
        what's its cardinality?
      </text>
      <choices>
        <correct>4</correct>
        <incorrect>3</incorrect>
        <incorrect>$\infty$</incorrect>
      </choices>
    </variation>
    <variation>
      <text>
        Given the set $A = \{x, y, z\}$,
        what's its cardinality?
      </text>
      <choices>
        <correct>3</correct>
        <incorrect>4</incorrect>
        <incorrect>$\infty$</incorrect>
      </choices>
    </variation>
  </question>

  <question>
    <!-- Q2 -->
    <variation_params>
      <variation>
        <param eye:name="setA">\{a, b, c, d\}</param>
        <param eye:name="correct">$\{a, d\}$</param>
        <param eye:name="incorrect1">$\{a, b, c, d, f\}$</param>
        <param eye:name="incorrect2">$\{f\}$</param>
      </variation>
      <variation>
        <param eye:name="setA">\{b, d, e, f\}</param>
        <param eye:name="correct">$\{d, f\}$</param>
        <param eye:name="incorrect1">$\{a, b, d, e, f\}$</param>
        <param eye:name="incorrect2">$\{a\}$</param>
      </variation>
    </variation_params>
    <text>
      What's the intersection of {{setA}} with $\{d, f, a\}$?
    </text>
    <choices>
      <correct>{{correct}}</correct>
      <incorrect>{{incorrect1}}</incorrect>
      <incorrect>{{incorrect2}}</incorrect>
    </choices>
  </question>

  <group>
    <common>
      <variation>
        <text>
          Consider the equation $2x + 3 = 7$:
        </text>
      </variation>
      <variation>
        <text>
          Consider the equation $3x + 2 = 11$:
        </text>
      </variation>
    </common>
    <question>
      <!-- Q3 -->
      <variation>
        <text>
          What's the solution to the equation?
        </text>
        <choices>
          <correct>$x=2$</correct>
          <incorrect>$x=-2$</incorrect>
          <incorrect>$x=0$</incorrect>
        </choices>
      </variation>
      <variation>
        <text>
          What's the solution to the equation?
        </text>
        <choices>
          <correct>$x=3$</correct>
          <incorrect>$x=-3$</incorrect>
          <incorrect>$x=0$</incorrect>
        </choices>
      </variation>
    </question>
    <question>
      <!-- Q4 -->
      <variation>
        <text>
          The equation is equivalent to:
        </text>
        <choices>
          <correct>$2x - 4 = 0$</correct>
          <incorrect>$x + 3 = 3$</incorrect>
          <incorrect>$2x + 10 = 0$</incorrect>
        </choices>
      </variation>
      <variation>
        <text>
          The equation is equivalent to:
        </text>
        <choices>
          <correct>$3x - 9 = 0$</correct>
          <incorrect>$x + 2 = 2$</incorrect>
          <incorrect>$3x + 13 = 0$</incorrect>
        </choices>
      </variation>
    </question>
  </group>

  <question>
    <!-- Q5 -->
    <text>
      The powerset of a set $A$ is:
    </text>
    <choices>
      <correct>
        The set whose elements are all the possible subsets of $A$.
      </correct>
      <incorrect>
        The cartesian product $A \times A$.
      </incorrect>
      <incorrect>
        The set $A \cup A$.
      </incorrect>
    </choices>
  </question>

  <question>
    <!-- Q6 -->
    <text>
      The set of all natural numbers is often denoted with:
    </text>
    <choices>
      <correct>
        $\mathbb{N}$
      </correct>
      <incorrect>
        $\mathbb{R}$
      </incorrect>
      <incorrect>
        $\mathbb{Z}$
      </incorrect>
    </choices>
  </question>

  <question>
    <!-- Q7 -->
    <variation>
      <text>
        The triangle of the following figure is:
      </text>
      <figure eye:width="0.2" eye:position="right">triangle-right.pdf</figure>
      <choices>
        <correct>A right triangle.</correct>
        <incorrect>An acute triangle.</incorrect>
        <incorrect>An obtuse triangle.</incorrect>
      </choices>
    </variation>
    <variation>
      <text>
        The triangle of the following figure is:
      </text>
      <figure eye:width="0.2" eye:position="right">triangle-acute.pdf</figure>
      <choices>
        <correct>An acute triangle.</correct>
        <incorrect>A right triangle.</incorrect>
        <incorrect>An obtuse triangle.</incorrect>
      </choices>
    </variation>
  </question>

</exam>
